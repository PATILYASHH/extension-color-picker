{
    "manifest_version": 3,
    "name": "Color Picker",
    "version": "1.0",
    "description": "A simple color picker extension",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png"
        
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  